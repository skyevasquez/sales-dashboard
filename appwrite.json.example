            {
              "key": "url",
              "type": "string",
              "size": 1000,
              "required": true,
              "default": null,
              "array": false
            },
            {
              "key": "storeIds",
              "type": "string",
              "size": 255,
              "required": true,
              "default": null,
              "array": true
