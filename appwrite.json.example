          "attributes": [
            {
              "key": "name",
              "type": "string",
              "size": 255,
              "required": true,
              "default": null,
              "array": false
            }
          ],
          "indexes": [],
          "$permissions": [
            "create(\"any\")",
            "read(\"any\")",
            "update(\"any\")",
            "delete(\"any\")"
          ]
        },
        {
          "name": "kpis",
          "$id": "kpis",
          "enabled": true,
          "documentSecurity": false,
          "attributes": [
            {
              "key": "name",
              "type": "string",
              "size": 255,
              "required": true,
              "default": null,
              "array": false
            }
          ],
          "indexes": [],
          "$permissions": [
            "create(\"any\")",
            "read(\"any\")",
            "update(\"any\")",
            "delete(\"any\")"
          ]
        },
        {
