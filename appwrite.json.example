              "key": "mtdSales",
              "type": "double",
              "required": true,
              "default": null,
              "array": false,
              "min": null,
              "max": null
            }
          ],
          "indexes": [
            {
              "key": "storeId_kpiId",
              "type": "unique",
              "status": "available",
              "attributes": ["storeId", "kpiId"],
              "orders": ["ASC", "ASC"]
